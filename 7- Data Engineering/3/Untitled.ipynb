    "def fibonacci(x):\n",
    "\tsequence_list = []\n",
    "\n",
    "\tcurrent = 0\n",
    "\tnext = 1\n",
    "\t\n",
    "\tfor i in range (x):\n",
    "\t\n",
    "\t\tsequence_list.append(current)\n",
    "\t\tcurrent = next\n",
    "\t\tif i > 0:\n",
    "\t\t\tnext = sequence_list[i] + current\n",
    "\t\telse:\n",
    "\t\t\tnext = 1\n",
    "\treturn sequence_list\t\n",
    "\t\n",
    "\n",
    "print(fibonacci(13))"
   ]
  }
